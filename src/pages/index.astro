---
import Burgermenu from "../components/Burgermenu.astro";
import Sektion1 from "../components/Sektion1.astro";
import Sektion2 from "../components/Sektion2.astro";
import Sektion3 from "../components/Sektion3.astro";
import Sektion4 from "../components/Sektion4.astro";
import Sektion5 from "../components/Sektion5.astro";
import Footer from "../components/Footer.astro";

import Layout from "../layouts/Layout.astro";
---

<Layout>
  <Burgermenu />
  <script
    src="https://unpkg.com/@dotlottie/player-component@2.7.12/dist/dotlottie-player.mjs"
    type="module"></script>
  <dotlottie-player
    src="https://lottie.host/ac6d3a8b-1f45-4da2-b966-f65153529ff4/oJGew1CKcx.lottie"
    background="transparent"
    speed="1"
    style="width: auto; height: auto; margin-top: 2rem; margin-bottom: 2rem; z-index: -1;"
    loop
    autoplay></dotlottie-player>
  <Sektion1 />
  <Sektion2 />
  <Sektion3 />
  <Sektion4 />
  <Sektion5 />
  <Footer />
</Layout>
